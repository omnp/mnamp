@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<urn:omnp:mnamp> a lv2:Plugin , lv2:AmplifierPlugin , doap:Project ;
    lv2:binary <mnamp.so> ;
    doap:name "mnamp" ;
    lv2:optionalFeature lv2:hardRTCapable ;

    lv2:port
        [
            a lv2:AudioPort , lv2:OutputPort ;
            lv2:index 0 ;
            lv2:symbol "out" ;
            lv2:name "Audio output" ;
        ] ,
        [
            a lv2:AudioPort , lv2:InputPort ;
            lv2:index 1 ;
            lv2:symbol "in" ;
            lv2:name "Audio input" ;
        ] ,
        [
            a lv2:InputPort , lv2:ControlPort ;
            lv2:index 2 ;
            lv2:symbol "gain" ;
            lv2:name "Gain" ;
            lv2:default 0.0 ;
            lv2:minimum -1.0 ;
            lv2:maximum 1.0 ;
        ] ,
        [
            a lv2:InputPort , lv2:ControlPort ;
            lv2:index 3 ;
            lv2:symbol "factor" ;
            lv2:name "Factor" ;
            lv2:default 1 ;
            lv2:minimum 1 ;
            lv2:maximum 4 ;
            lv2:portProperty lv2:integer ;
        ] ,
        [
            a lv2:InputPort , lv2:ControlPort ;
            lv2:index 4 ;
            lv2:symbol "drive1" ;
            lv2:name "Drive 1" ;
            lv2:default 3.0 ;
            lv2:minimum 0.01 ;
            lv2:maximum 11.0 ;
        ] ,
        [
            a lv2:InputPort , lv2:ControlPort ;
            lv2:index 5 ;
            lv2:symbol "drive2" ;
            lv2:name "Drive 2" ;
            lv2:default 1.0 ;
            lv2:minimum 0.01 ;
            lv2:maximum 11.0 ;
        ] .
